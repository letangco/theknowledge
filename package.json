    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@babel/register": "^7.9.0",
        "ava": "^0.15.2",
        "chai": "^3.5.0",
        "coveralls": "^2.11.9",
        "enzyme": "^2.3.0",
        "eslint": "^2.11.1",
        "eslint-config-airbnb": "^9.0.1",
        "eslint-plugin-ava": "^2.4.0",
        "eslint-plugin-import": "^1.8.1",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eventsource-polyfill": "^0.9.6",
        "grunt-apidoc": "^0.11.0",
        "jsdom": "^9.2.1",
        "log": "^1.4.0",
        "mockgoose": "^6.0.3",
        "nock": "^8.0.0",
        "nodemon": "^1.9.2",
        "null-loader": "^0.1.1",
        "nyc": "^6.4.4",
        "pre-commit": "^1.1.3",
        "rimraf": "^2.5.2",
        "sinon": "^1.17.4",
        "supertest": "^1.2.0",
        "xo": "^0.18.2"
    },
    "engines": {
        "node": ">=8"
    },
    "ava": {
        "files": [
            "server/**/*.spec.js"
        ],
        "source": [
            "server/**/*.js"
        ],
        "failFast": true,
        "babel": "inherit",
        "require": []
    },
    "nyc": {
        "include": [
            "server/**/*.js"
        ],
        "exclude": [
            "**/*.spec.js",
            "server/util/test-helpers.js",
            "server/config.js",
            "server/dummyData.js"
        ],
        "reporter": [
            "lcov",
            "text",
            "html"
        ]
    }
