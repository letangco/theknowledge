{
    "name": "benri-backend",
    "version": "2.0.0",
    "description": "Benri backend service",
    "scripts": {
        "dev": "nodemon server.js",
        "debug": "nodemon --inspect-brk=0.0.0.0 server.js",
        "prod": "pm2 start server.js --name benri-backend",
        "slate": "rimraf node_modules && npm install",
        "lint": "eslint server",
        "docs": "apidoc -i server/docs/ -o server/docs_output",
        "elasticsearch": "node elasticsearch/elasticsearch.js",
        "cloud": "cross-env NODE_ENV=production pm2 start cloud/cloud.js -i 2 --name cloud",
        "start:cloud": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon cloud/cloud.js",
        "worker": "cross-env NODE_ENV=production pm2 start worker/worker.js -i 2 --name worker",
        "start:worker": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon worker/worker.js",
        "hook": "cross-env NODE_ENV=production pm2 start room-hook/room-hook.entry.js -i 2 --name hook",
        "start:hook": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon room-hook/room-hook.entry.js",
        "clean:hook": "node hook-scripts/hook.entry.js"
    },
    "homepage": "https://benri.education",
    "author": "Tesse team",
    "private": true,
    "dependencies": {
        "@babel/polyfill": "^7.8.7",
        "@babel/runtime": "^7.9.2",
        "@sendgrid/mail": "^6.1.4",
        "@tggs/core-authencation": "^1.0.4",
