        "languagedetect": "^1.1.1",
        "libreoffice-convert": "^1.0.5",
        "limax": "^1.3.0",
        "lodash": "^4.17.4",
        "memory-cache": "^0.2.0",
        "meta-scrape": "0.0.4",
        "mkdirp-promise": "^5.0.1",
        "moment": "^2.17.0",
