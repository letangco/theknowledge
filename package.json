        "cross-env": "^1.0.8",
        "cuid": "^1.3.8",
        "currency-formatter": "^1.5.1",
        "dotenv": "^8.2.0",
        "elasticsearch": "^13.0.0-rc2",
        "email-validator": "^2.0.3",
        "execa": "^5.0.0",
        "express": "^4.13.4",
        "express-validator": "^6.4.0",
        "filereader": "^0.10.3",
